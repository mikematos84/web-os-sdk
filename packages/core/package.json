{
  "name": "@web-os/core",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.react.esm.js",
      "require": "./dist/index.react.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./standalone": {
      "import": "./dist/index.standalone.esm.js",
      "require": "./dist/index.standalone.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./umd": "./dist/index.umd.js"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "reset": "pnpm run clean && rimraf node_modules"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@mui/material": "^6.0.0",
    "@mui/icons-material": "^6.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.0.0",
    "@rollup/plugin-replace": "^5.0.0"
  }
}
